 ] $i2s_transmitter_0

  # Create instance: mem_interconnect, and set properties
  set mem_interconnect [ create_bd_cell -type ip -vlnv xilinx.com:ip:axi_interconnect:2.1 mem_interconnect ]
  set_property -dict [ list \
